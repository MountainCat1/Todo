{
  "Routes": [
    {
      "DownstreamPathTemplate": "/Todo{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "todo-api",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/api/Todo{everything}",
      "UpstreamHttpMethod": [ "POST", "PUT", "GET", "DELETE" ],
      "DangerousAcceptAnyServerCertificateValidator": true,
      "SwaggerKey": "todos"
    },
    {
      "DownstreamPathTemplate": "/Teams{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "teams-api",
          "Port": 5002
        }
      ],
      "UpstreamPathTemplate": "/api/Teams{everything}",
      "UpstreamHttpMethod": [ "POST", "PUT", "GET", "DELETE" ],
      "DangerousAcceptAnyServerCertificateValidator": true,
      "SwaggerKey": "teams"
    },
    {
      "DownstreamPathTemplate": "/Authentication{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "authentication-api",
          "Port": 5004
        }
      ],
      "UpstreamPathTemplate": "/api/Authentication{everything}",
      "UpstreamHttpMethod": [ "POST", "PUT", "GET", "DELETE" ],
      "DangerousAcceptAnyServerCertificateValidator": true,
      "SwaggerKey": "authentication"
    },
    {
      "DownstreamPathTemplate": "/User{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "authentication-api",
          "Port": 5004
        }
      ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": []
      },
      "UpstreamPathTemplate": "/api/User{everything}",
      "UpstreamHttpMethod": [ "POST", "PUT", "GET", "DELETE" ],
      "DangerousAcceptAnyServerCertificateValidator": true,
      "SwaggerKey": "users"
    },
    {
      "DownstreamPathTemplate": "/RabbitMQ{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "rabbitmq",
          "Port": 5672
        }
      ],
      "UpstreamPathTemplate": "/api/RabbitMQ{everything}",
      "UpstreamHttpMethod": [ "POST", "PUT", "GET", "DELETE" ],
      "DangerousAcceptAnyServerCertificateValidator": true,
      "SwaggerKey": "todos"
    }
  ],
  "GlobalConfiguration": {
    "RequestIdKey": "OcRequestId",
    "AdministrationPath": "/administration",
    "RouteIsCaseSensitive": true
  },
  "SwaggerEndPoints": [
    {
      "Key": "authentication",
      "Config": [
        {
          "Name": "Authentication API",
          "Version": "v1",
          "Url": "https://authentication-api/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "teams",
      "Config": [
        {
          "Name": "Teams API",
          "Version": "v1",
          "Url": "https://teams-user/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "todos",
      "Config": [
        {
          "Name": "Todos API",
          "Version": "v1",
          "Url": "https://todos-user/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}
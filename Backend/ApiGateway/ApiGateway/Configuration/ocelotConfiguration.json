{

  "Routes": [
    {
      "DownstreamPathTemplate": "/Todo",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "todo-api",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/api/Todo",
      "UpstreamHttpMethod": [ "POST", "PUT", "GET", "DELETE" ],
      "DangerousAcceptAnyServerCertificateValidator": true
    },
    {
      "DownstreamPathTemplate": "/Teams",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "teams-api",
          "Port": 5002
        }
      ],
      "UpstreamPathTemplate": "/api/Teams",
      "UpstreamHttpMethod": [ "POST", "PUT", "GET", "DELETE" ],
      "DangerousAcceptAnyServerCertificateValidator": true
    },
    {
      "DownstreamPathTemplate": "/Authentication/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "authentication-api",
          "Port": 5004
        }
      ],
      "UpstreamPathTemplate": "/api/Authentication/{everything}",
      "UpstreamHttpMethod": [ "POST", "PUT", "GET", "DELETE" ],
      "DangerousAcceptAnyServerCertificateValidator": true
    }
  ],
  "GlobalConfiguration": {
    "RequestIdKey": "OcRequestId",
    "AdministrationPath": "/administration",
    "RouteIsCaseSensitive": true
  }
}